<!-- index.hbs -->
<h1>Cat Reservation</h1>
<link rel="stylesheet" href="/css/style.css">

<table>
  <thead>
    <tr>
      <th>Cat ID</th>
      <th>Reservation ID</th>
      <th></th>
    </tr>
  </thead>
{{#each cat_reservation}}
  <tr>
    <td>{{cat_id}}</td>
    <td>{{reservation_id}}</td>
    <td><button onclick="deleteCatReservation({{id}})">Delete</button></td>
  </tr>
{{/each}}
</table>